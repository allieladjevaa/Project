<Window x:Class="Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project"
        mc:Ignorable="d"
        Title="MainPage" Height="480" Width="250">
    <Grid Background="#FFFFB0B0">
        <TextBox HorizontalAlignment="Center" Margin="0,83,0,0" Text="Username" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <PasswordBox HorizontalAlignment="Center" Margin="0,158,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Log In" HorizontalAlignment="Center" Margin="0,263,0,0" VerticalAlignment="Top" Background="White" Width="82"/>
        <Button Content="Sign up" HorizontalAlignment="Center" Margin="0,313,0,0" VerticalAlignment="Top" Background="White" Width="82"/>

    </Grid>
</Window>
